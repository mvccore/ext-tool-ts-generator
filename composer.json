{
	"name": "mvccore/ext-tool-ts-generator",
	"type": "library",
	"description": "MvcCore - Extension - Tool - TypeScript Generator - utility to easilly generate TypeScript classes, interfaces or enums from PHP equivalents.",
	"keywords": [
		"mvc", 
		"mvccore", 
		"framework", 
		"extension", 
		"plugin", 
		"plug-in", 
		"ext", 
		"tool", 
		"typescript",
		"ts", 
		"generator", 
		"interface", 
		"class", 
		"enum"
	],
	"license": ["BSD-3-Clause"],
	"authors": [{
		"name": "Tom Flidr",
		"homepage": "https://github.com/tomFlidr"
	}],
	"require": {
		"php": ">=5.4.0",
		"mvccore/mvccore": "^5.3"
	},
	"require-dev": {
		"php": ">=5.4.0",
		"nette/tester": "<=2.4",
		"tomflidr/tracy": "<=2.5.13"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}